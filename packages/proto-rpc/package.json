{
  "name": "@streamr/proto-rpc",
  "version": "103.1.2",
  "description": "Proto-RPC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamr-dev/network.git",
    "directory": "packages/proto-rpc"
  },
  "main": "dist/src/exports.js",
  "types": "dist/src/exports.d.ts",
  "files": [
    "dist",
    "!*.tsbuildinfo",
    "README.md",
    "LICENSE"
  ],
  "license": "(Apache-2.0 AND BSD-3-Clause)",
  "author": "Streamr Network AG <contact@streamr.network>",
  "scripts": {
    "prebuild": "./proto.sh",
    "build": "tsc -b tsconfig.node.json",
    "build-browser": "webpack --mode=development --progress",
    "build-protos": "./proto.sh && ./test-proto.sh",
    "check": "npm run build-protos && tsc -p ./tsconfig.jest.json",
    "clean": "jest --clearCache || true; rm -rf dist *.tsbuildinfo node_modules/.cache || true",
    "eslint": "npm run build-protos && eslint --cache --cache-location=node_modules/.cache/.eslintcache/ '*/**/*.{js,ts}'",
    "test": "npm run test-unit && npm run test-integration",
    "test-browser": "npm run build-protos && karma start karma.config.js",
    "test-unit": "npm run build-protos && jest test/unit",
    "test-integration": "npm run build-protos && jest --bail test/integration"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.8.2",
    "@protobuf-ts/runtime-rpc": "^2.8.2",
    "@streamr/utils": "103.1.2",
    "eventemitter3": "^5.0.0",
    "lodash": "^4.17.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@streamr/browser-test-runner": "^0.0.1",
    "@streamr/test-utils": "103.1.2",
    "@types/lodash": "^4.17.21"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
